<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERR">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss.SSS}] %level by %logger{36}.%M: %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="err">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>